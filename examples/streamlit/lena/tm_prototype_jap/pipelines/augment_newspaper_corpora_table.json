        "constants": {
          "column_name": "file_name"
        }
      },
      "step_id": "extract_file_name_column"
    },
    {
      "module_type": "playground.markus.topic_modeling.extract_date_and_pub_ref",
      "module_config": {
        "constants": {
          "pub_name_replacement_map": {
            "sn20211019": "Allgemeine_Zeitschrift",
            "sn20211018": "Monatsschrift_Psychiatrie",
            "sn20211016": "Meiji",
            "sn20211020": "Galactic_Railroad",
            "86655700": "American_Journal_Insanity"
          }
        }
      },
      "step_id": "extract_date_and_pub_ref",
      "input_links": {
